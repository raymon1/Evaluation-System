		 <form method="post" action="/quiz/create">
    <div class="container-fluid" >

	<div class="row">


		<div class="col-md-8">
		     <div class="panel panel-default">
		                 <div class="panel-body">
		                     <div align= "middle" class="page-header">
		                         <h3> New Quiz</h3>
		                     </div>

							 <div id="question_add">

							 </div>


								<div class="col-md-4" align= "middle">
<%= link_to "Add MCQ" ,{controller: "quizzes",action: "add_mcq"},remote: true ,:type=>"button",:class => "button btn btn-success btn-default"%>
				</div>
				<div class="col-md-4" align="middle">
<%= link_to "Add True or false",{controller: "quizzes",action: "add_tf"},remote: true ,:type=>"button",:class => "button btn btn-success btn-default"%>
				</div>
				<div class="col-md-4" align = "middle">
<%= link_to "Add Essay",{controller: "quizzes",action: "add_essay"},remote: true ,:type=>"button",:class => "button btn btn-success btn-default"%>
				</div>               
				<br> </br>     
				<br> </br>            
							   <!-- till here -->
							   <div align="middle">
			<%= button_to "submit", {controller: "quizzes",action:"create"} ,{:type => "submit",:class => "btn btn-primary"}%>
							 </div>

                                     <p><br/></p>
		                 </div>
		     </div>
		</div>
		<div class="col-md-4">
				<%= render partial: "forms/quizInfo" %>
		</div>

		                            

	</div>
</div>
</form>


